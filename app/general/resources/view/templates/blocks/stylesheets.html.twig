{% stylesheets
	'@<Add vendor name>:General::resources/assets/css/reset.css'
	'@<Add vendor name>:General::resources/assets/css/basic.css'
	'@<Add vendor name>:General::resources/assets/css/layout.css'
	'@<Add vendor name>:General::resources/assets/css/fonts.css'
	'@<Add vendor name>:General::resources/assets/css/checkout.css'
	'@<Add vendor name>:General::resources/assets/css/account.css'
	'@<Add vendor name>:General::resources/assets/css/styles.css'
	'@<Add vendor name>:General::resources/assets/css/mobile.css'
	output='/assets/css/<Add vendor name>-*.css'
	filter='csscogulerewrite,?cssmin'
%}
<link rel="stylesheet" href="{{ asset_url }}">
{% endstylesheets %}

{% stylesheets
	'@<Add vendor name>:General::resources/assets/css/ie8.css'

	output='/assets/css/ie8.css'
	filter='csscogulerewrite,?cssmin'
%}
